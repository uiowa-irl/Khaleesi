{
	"name": "Khaleesi",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"persistent": true,
		"scripts": [
			"background/lib/psl.js",
			"background/model.js",
			"background/encode.js",
			"background/chains.js",
			"background/featureExtraction.js",
			"background/background.js"
		]
	},
	"content_scripts": [
		{
			"all_frames": true,
			"js": [
				"content/throwStack.js"
			],
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"tabs",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	]
}